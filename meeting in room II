#include <bits/stdc++.h>
int minRooms(vector<vector<int>> intervals) {
 int i=0,j=0;
 int len=intervals.size();
 if (len == 0) return 0;
 vector<int>arrival(len);
 vector<int>departure(len);
 for (int i = 0; i < len; i++) {
        arrival[i] = intervals[i][0];
        departure[i] = intervals[i][1];
    }
sort(arrival.begin(),arrival.end());
sort(departure.begin(),departure.end());
int cnt=0;
int maxi=INT_MIN;
while(i<len){
    if(arrival[i]<departure[j]){
        cnt++;
        i++;
    } else {
        cnt--;
        j++;
    }
    maxi = max(maxi, cnt);
}
return maxi;
}
